<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #91a65f;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.users.CenterCoordinatorGUI">
   <top>
      <Label text="Center Coordinator Menu" BorderPane.alignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </top>
   <center>
      <VBox alignment="CENTER" prefHeight=".0" prefWidth="100.0" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#selectExportStats" style="-fx-background-color: #dddea0;" text="Check and Export vaccination statistics" />
            <Button fx:id="performanceButton" mnemonicParsing="false" onAction="#selectAnalyseCenterPerformance" style="-fx-background-color: #dddea0;" text="Analyze performance of a center" />
            <Button fx:id="importDatabutton" mnemonicParsing="false" onAction="#selectImportLegacySystemData" style="-fx-background-color: #dddea0;" text="Import Legacy Data" />
         </children>
      </VBox>
   </center>
   <bottom>
      <Button cancelButton="true" layoutX="200.0" minWidth="80.0" mnemonicParsing="false" onAction="#stop" style="-fx-background-color: #dddea0;" text="Cancel" BorderPane.alignment="BOTTOM_RIGHT" />
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
